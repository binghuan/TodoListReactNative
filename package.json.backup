{
  "name": "todolist-react-native",
  "version": "1.0.0",
  "description": "A simple and elegant Todo List app built with React Native",
  "main": "index.js",
  "scripts": {
    "android": "export JAVA_HOME=/Library/Java/JavaVirtualMachines/temurin-17.jdk/Contents/Home && react-native run-android",
    "ios": "react-native run-ios",
    "macos": "react-native run-macos",
    "start": "react-native start",
    "clean": "react-native clean-project-auto",
    "test": "jest",
    "lint": "eslint .",
    "build:android": "export JAVA_HOME=/Library/Java/JavaVirtualMachines/temurin-17.jdk/Contents/Home && cd android && ./gradlew assembleRelease",
    "build:ios": "react-native run-ios --configuration Release",
    "build:macos": "react-native run-macos --configuration Release"
  },
  "keywords": [
    "react-native",
    "todo",
    "todolist",
    "mobile",
    "desktop",
    "cross-platform"
